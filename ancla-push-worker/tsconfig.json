{
    "compilerOptions": {
        "target": "ES2021",
        "module": "ES2022",
        "moduleResolution": "bundler",
        "lib": [
            "ES2021"
        ],
        "types": [
            "@cloudflare/workers-types"
        ],
        "strict": true,
        "outDir": "dist",
        "rootDir": "src",
        "skipLibCheck": true,
        "esModuleInterop": true
    },
    "include": [
        "src/**/*"
    ]
}